{
  "name": "my-electron-app",
  "version": "0.1.0",
  "description": "Hello GUI EXE with Electron",
  "main": "main.js",
  "author": "Goeun",
  "license": "MIT",
  "scripts": {
    "dev": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.goeun.my-electron-app",
    "productName": "MyElectronApp",
    "files": [
      "main.js",
      "preload.js",
      "renderer/**",
      "renderer/dist/**",
      "assets/**",
      "package.json"
    ],
    "win": {
      "target": "portable"
    }
  },
  "devDependencies": {
    "electron": "^32.0.0",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
